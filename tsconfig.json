{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@asset/*": [
        "./src/asset/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@screens/*": [
        "src/screens/*"
      ],
      "@theme/*": [
        "./src/theme/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
    }
  }
}
